		"@sveltejs/vite-plugin-svelte": "^3.1.2",
		"@types/eslint": "^9.6.1",
		"@typescript-eslint/eslint-plugin": "^8.8.0",
		"@typescript-eslint/parser": "^8.8.0",
		"autoprefixer": "^10.4.20",
		"eslint": "^9.11.1",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.44.1",
		"flowbite": "2.5.2",
